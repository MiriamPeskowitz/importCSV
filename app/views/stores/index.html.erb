<%= flash[:notice] %>
<table>
	<thead>
	  <tr>
		<th>Name</th>
		<th>Address1</th>
		<th>Zip Code</th>
		<th>Phone Number</th>
		<th>Website</th>
		<th>Comments</th>
		<th>Visited </th>
		<th>Lat</th>
		<th>Long</th>
	  </tr>
	</thead>
	<tbody>
		<% @stores.each do |store| %>
		<tr>
			<td><%= store.name %></td>
			<td><%= store.address1 %></td>
			<td><%= store.address2 %></td>
			<td><%= store.zip_code %></td>
			<td><%= store.phone_number %></td>
			<td><%= store.website %></td>
			<td><%= store.comments %></td>
			<td><%= store.visited %></td>
			<td><%= store.lat %></td>
			<td><%= store.long %></td>
		</tr>
		<% end %>
	</tbody>
</table>

<div>
	<h4>Import the data!</h4>
	 <%= form_tag import_stores_path, multipart: true do %>
	 	<%= file_field_tag :file %>
	 	<%= submit_tag "Import CSV" %>
	 <% end %>
</div>
</div>
